@model Manager
@{
    ViewData["Title"] = "Delete Manager";
}
<h2>
    Delete Manager?
</h2>
<div class="d-flex flex-row">
    <form asp-controller="Managers" asp-action="Delete" method="post">
        <input hidden asp-for="Id" />

        <div class="mb-3">
            <input type="submit" class="btn btn-danger" value="Delete" />
        </div>
    </form>
    <form asp-controller="Managers" asp-action="Index" method="post">
        <input hidden asp-for="Id" />

        <div class="mb-3">
            <input type="submit" class="btn btn-warning" value="Cancel" />
        </div>
    </form>
</div>